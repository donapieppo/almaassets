<%= main_title('Utenti') %>

<% initial = '' %>  
<ul class="users-list">
  <% @users.each do |user| %>  
    <% if initial != (initial = user.surname[0]) %>  
      <li class="users-list-letter"><%= initial %></li>
    <% end %>
    <li><%= link_to dm_icon("wallet", text: user.cn_militar), user_goods_path(user) %></li>
  <% end %>
</ul>
